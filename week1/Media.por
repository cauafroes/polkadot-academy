programa {
  funcao inicio() {
    real n1, n2, n3, avg

    escreva("Digite o primeiro número: ")
    leia(n1)

    escreva("Digite o segundo número: ")
    leia(n2)

    escreva("Digite o terceiro número: ")
    leia(n3)

    avg = n1 + n2 + n3 / 3
    
    escreva("A média deles é: ", avg)
  }
}
